<PropertyList>
    
      <path>Aircraft/CSeries/Models/CS100.ac</path>
  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/CSeries/Models/Liveries", 10);
    </load>

    <unload>
      livery_update.stop();
    </unload>
  </nasal>

	
	<model>
		<path>Aircraft/CSeries/Models/Cockpit/flightdeck.xml</path>
		<offsets>
			<x-m>-14.5</x-m>
			<y-m>0</y-m>
			<z-m>-2</z-m>
		</offsets>
	</model>
	
	
  <effect>
      <inherits-from>Aircraft/CSeries/Models/Effects/reflect</inherits-from>
    <object-name>body</object-name>
    <object-name>wings</object-name>
    <object-name>Circle</object-name>
    <object-name>hstab</object-name>
    <object-name>vstab</object-name>
    <object-name>doorFL</object-name>
    <object-name>doorFR</object-name>
    <object-name>doorRL</object-name>
    <object-name>doorRR</object-name>
    <object-name>nacelleL</object-name>
    <object-name>nacelleR</object-name>
    <object-name>enginefix.</object-name>
  </effect>
  <effect>
       <inherits-from>Aircraft/CSeries/Models/Effects/reflectglass</inherits-from>
         <object-name>windows</object-name>
     </effect> 
  <!--LIVERY-->
  <animation>
    <type>material</type>
    <object-name>body</object-name>
    <object-name>wings</object-name>
    <object-name>Circle</object-name>
    <object-name>hstab</object-name>
    <object-name>vstab</object-name>
    <object-name>doorFL</object-name>
    <object-name>doorFR</object-name>
    <object-name>doorRL</object-name>
    <object-name>doorRR</object-name>
    
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
    <specular>
      <red-prop>specular/red</red-prop>
      <green-prop>specular/green</green-prop>
      <blue-prop>specular/blue</blue-prop>
    </specular>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>nacelleL</object-name>
    <object-name>nacelleR</object-name>
    
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture-engine</texture-prop>
    <texture>texture-engine.png</texture>
    <specular>
      <red-prop>specular/red</red-prop>
      <green-prop>specular/green</green-prop>
      <blue-prop>specular/blue</blue-prop>
    </specular>
  </animation>

	
	<animation>
		<name>FanL</name>
		<type>spin</type>
		<object-name>fanL</object-name>
		<object-name>spinnerL</object-name>
                <property>engines/engine/n1</property>
		<factor>67</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
                <center>
			<y-m>-5.536</y-m>
			<z-m>-4.01626</z-m>
                    </center>
	</animation>
	<animation>
		<name>FanR</name>
		<type>spin</type>
		<object-name>fanR</object-name>
		<object-name>spinnerR</object-name>
                <property>engines/engine/n1</property>
		<factor>67</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
                <center>
			<y-m>5.536</y-m>
			<z-m>-4.01626</z-m>
                    </center>
	</animation>
