	</action>
    </animation>
    <animation>
	<type>textranslate</type>
	<object-name>lgen</object-name>
		<property>controls/electric/engine[0]/generator</property>
		<factor>0.025</factor>
  		<axis>
  	  		<x>1</x>
  	 		 <y>0</y>
  	  		<z>0</z>
  		</axis>
    </animation>
    <animation>
	<type>pick</type>
	<object-name>rgen</object-name>
	<action>
	    <button>0</button>
