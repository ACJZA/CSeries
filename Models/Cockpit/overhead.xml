  		</axis>
    </animation>
    <animation>
	<type>texmultiple</type>
	<object-name>apu</object-name>
<!--While running-->
  		<transform>
   		 <subtype>texrotate</subtype>
		<property>controls/APU/knob</property>
		<factor>61.2</factor>
		<center>
			<x>0.09</x>
			<y>0.205</y>
		</center>
  		<axis>
  	  		<x>0</x>
  	 		 <y>0</y>
  	  		<z>1</z>
  		</axis>
 		 </transform>
<!--While starting-->
  		<transform>
   		 <subtype>texrotate</subtype>
		<property>controls/APU/knob2</property>
		<factor>120.81</factor>
		<center>
			<x>0.09</x>
			<y>0.205</y>
		</center>
  		<axis>
  	  		<x>0</x>
  	 		 <y>0</y>
  	  		<z>1</z>
  		</axis>
 		 </transform>
    </animation>
    <animation>
	<type>pick</type>
	<object-name>apugen</object-name>
	<action>
	    <button>0</button>
	    <repeatable type="bool">false</repeatable>
	    <binding>
