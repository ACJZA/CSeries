    </animation>
    <animation>
	<type>textranslate</type>
	<object-name>rgen</object-name>
		<property>controls/electric/engine[1]/generator</property>
		<factor>0.025</factor>
  		<axis>
  	  		<x>1</x>
  	 		 <y>0</y>
  	  		<z>0</z>
  		</axis>
    </animation>
    <animation>
	<type>pick</type>
	<object-name>apu</object-name>
	<action>
	    <button>0</button>
	    <repeatable type="bool">false</repeatable>
	    <binding>
		<command>property-toggle</command>
		<property>controls/APU/electronic-control-unit</property>
	    </binding>
	    <binding>
		<command>property-toggle</command>
		<property>controls/APU/off-on</property>
	    </binding>
	</action>
    </animation>
<!--BLEED-->

    <!--Generators-->
    <animation>
	<type>pick</type>
	<object-name>lbleed</object-name>
	<action>
	    <button>0</button>
	    <repeatable type="bool">false</repeatable>
