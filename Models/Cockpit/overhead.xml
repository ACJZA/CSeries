<?xml version="1.0" encoding="UTF-8" ?>

<!-- Bombardier C-series -->
<!-- Overhead Panel -->

<PropertyList>

	<path>overhead.ac</path>

	<panel-marking-emit>
		<red>1</red>
		<green>0.871</green>
		<blue>0.427</blue>
	</panel-marking-emit>

	<!-- Visible only within 1/2 km -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>500</max-m>
	</animation>
        <!--ELECTRICAL-->
<!--Cabin Power-->
    <animation>
	<type>pick</type>
	<object-name>cabinpwr</object-name>
	<action>
	    <button>0</button>
	    <repeatable type="bool">false</repeatable>
	    <binding>
		<command>property-toggle</command>
		<property>controls/electric/cabin-power</property>
	    </binding>
	</action>
    </animation>
    <animation>
	<type>textranslate</type>
	<object-name>cabinpwr</object-name>
		<property>controls/electric/cabin-power</property>
		<factor>0.025</factor>
  		<axis>
  	  		<x>1</x>
  	 		 <y>0</y>
  	  		<z>0</z>
  		</axis>
    </animation>
<!--Ram Air Turbine Generator-->
    <animation>
	<type>pick</type>
	<object-name>ratgen</object-name>
	<action>
	    <button>0</button>
	    <repeatable type="bool">false</repeatable>
	    <binding>
		<command>property-toggle</command>
		<property>controls/pneumatic/ram-air-turbine</property>
	    </binding>
	</action>
    </animation>
    <animation>
	<type>textranslate</type>
	<object-name>ratgen</object-name>
		<property>controls/pneumatic/ram-air-turbine</property>
		<factor>0.025</factor>
  		<axis>
  	  		<x>1</x>
  	 		 <y>0</y>
  	  		<z>0</z>
  		</axis>
    </animation>

        <!--Batteries-->
    
    <animation>
	<type>pick</type>
	<object-name>batt1</object-name>
	<action>
	    <button>0</button>
	    <repeatable type="bool">false</repeatable>
	    <binding>
		<command>property-toggle</command>
		<property>controls/electric/battery-switch</property>
	    </binding>
	</action>
    </animation>
    <animation>
	<type>texrotate</type>
	<object-name>batt1</object-name>
		<property>controls/electric/battery-switch</property>
		<factor>55</factor>
		<center>
			<x>0.09</x>
			<y>0.205</y>
		</center>
  		<axis>
  	  		<x>0</x>
  	 		 <y>0</y>
  	  		<z>1</z>
  		</axis>
    </animation>
    <animation>
	<type>pick</type>
	<object-name>batt2</object-name>
	<action>
	    <button>0</button>
	    <repeatable type="bool">false</repeatable>
	    <binding>
		<command>property-toggle</command>
		<property>controls/electric/battery-switch[1]</property>
	    </binding>
	</action>
    </animation>
    <animation>
	<type>texrotate</type>
	<object-name>batt2</object-name>
		<property>controls/electric/battery-switch[1]</property>
		<factor>55</factor>
		<center>
			<x>0.09</x>
			<y>0.205</y>
		</center>
  		<axis>
  	  		<x>0</x>
  	 		 <y>0</y>
  	  		<z>1</z>
  		</axis>
    </animation>
