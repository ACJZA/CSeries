	    </binding>
	</action>
    </animation>
    <animation>
	<type>texmultiple</type>
        <object-name>extpwr</object-name>
<!--AVAIL-->
  		<transform>
   		 <subtype>textranslate</subtype>
		<property>controls/ext-avail</property>
		<factor>0.025</factor>
  		<axis>
  	  		<x>1</x>
  	 		 <y>0</y>
  	  		<z>0</z>
  		</axis>
 		 </transform>
<!--IN USE-->
