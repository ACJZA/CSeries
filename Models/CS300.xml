<PropertyList>
    
      <path>Aircraft/CSeries/Models/CS300.ac</path>
  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/CSeries/Models/Liveries/CS300", 10);
    </load>

    <unload>
      livery_update.stop();
    </unload>
  </nasal>

  <!--All Models share the same flightdeck--> 
	<model>
		<path>Aircraft/CSeries/Models/Cockpit/flightdeck.xml</path>
		<offsets>
			<x-m>-16.4</x-m>
			<y-m>0</y-m>
			<z-m>-2.1</z-m>
		</offsets>
	</model>
	
	
  <effect>
      <inherits-from>Aircraft/CSeries/Models/Effects/reflect</inherits-from>
    <object-name>body</object-name>
    <object-name>wings</object-name>
    <object-name>Circle</object-name>
    <object-name>hstab</object-name>
    <object-name>vstab</object-name>
    <object-name>doorFL</object-name>
    <object-name>doorFR</object-name>
    <object-name>doorRL</object-name>
    <object-name>doorRR</object-name>
    <object-name>nacelleL</object-name>
    <object-name>nacelleR</object-name>
    <object-name>enginefix.</object-name>
  </effect>
  <effect>
       <inherits-from>Aircraft/CSeries/Models/Effects/reflectglass</inherits-from>
         <object-name>windows</object-name>
     </effect> 
  <!--LIVERY-->
  <animation>
    <type>material</type>
    <object-name>body</object-name>
    <object-name>wings</object-name>
    <object-name>Circle</object-name>
    <object-name>hstab</object-name>
    <object-name>vstab</object-name>
    <object-name>doorFL</object-name>
    <object-name>doorFR</object-name>
    <object-name>doorRL</object-name>
    <object-name>doorRR</object-name>
    
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
    <specular>
      <red-prop>specular/red</red-prop>
      <green-prop>specular/green</green-prop>
      <blue-prop>specular/blue</blue-prop>
    </specular>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>nacelleL</object-name>
    <object-name>nacelleR</object-name>
    
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture-engine</texture-prop>
    <texture>texture-engine.png</texture>
    <specular>
      <red-prop>specular/red</red-prop>
      <green-prop>specular/green</green-prop>
      <blue-prop>specular/blue</blue-prop>
    </specular>
  </animation>

	<animation>
		<type>shader</type>
        <shader>chrome</shader>
        <texture>chrome.png</texture>
		<object-name>apu.chrome</object-name>
	</animation>
	
	<animation>
		<name>FanL</name>
		<type>spin</type>
		<object-name>fanL</object-name>
		<object-name>spinnerL</object-name>
                <property>engines/engine/n1</property>
		<factor>67</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
                <center>
			<y-m>-5.536</y-m>
			<z-m>-4.01626</z-m>
                    </center>
	</animation>
	<animation>
		<name>FanR</name>
		<type>spin</type>
		<object-name>fanR</object-name>
		<object-name>spinnerR</object-name>
                <property>engines/engine/n1</property>
		<factor>67</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
                <center>
			<y-m>5.536</y-m>
			<z-m>-4.01626</z-m>
                    </center>
	</animation>
        <!--GEAR-->
        <!--Compression-->
    <animation>
		<type>translate</type>
		<object-name>wheelF</object-name>
		<object-name>gearFD</object-name>
		<object-name>gearFDdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>1</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>gearFDdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>80</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-15.76147</x-m>
			<z-m>-5.13845</z-m>
                    </center>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>gearFUdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>-80</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-15.61225</x-m>
			<z-m>-4.50552</z-m>
                    </center>
	</animation>
        <!--Left main-->
     <animation>
		<type>translate</type>
		<object-name>wheelL</object-name>
		<object-name>gearLD</object-name>
		<object-name>gearLDdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>1</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>gearLDdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>80</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-0.53269</x-m>
			<z-m>-5.06946</z-m>
                    </center>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>gearLUdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>-80</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-0.38197</x-m>
			<z-m>-4.44865</z-m>
                    </center>
	</animation>
        <!--LRight main-->
     <animation>
		<type>translate</type>
		<object-name>wheelR</object-name>
		<object-name>gearRD</object-name>
		<object-name>gearRDdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>1</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>gearRDdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>80</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-0.53269</x-m>
			<z-m>-5.06946</z-m>
                    </center>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>gearRUdamper</object-name>
                <property>gear/gear/compression-m</property>
		<factor>-80</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-0.38197</x-m>
			<z-m>-4.44865</z-m>
                    </center>
	</animation>
        <!--Wheel spinning-->
    <animation>
		<type>spin</type>
		<object-name>wheelF</object-name>
                <property>gear/gear/rollspeed-ms</property>
		<factor>10</factor>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-15.75911</x-m>
			<z-m>-5.27204</z-m>
                    </center>
	</animation>
    <animation>
		<type>spin</type>
		<object-name>wheelL</object-name>
                <property>gear/gear[1]/rollspeed-ms</property>
		<factor>10</factor>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-0.55741</x-m>
			<z-m>-5.20529</z-m>
                    </center>
	</animation>
    <animation>
		<type>spin</type>
		<object-name>wheelR</object-name>
                <property>gear/gear[2]/rollspeed-ms</property>
		<factor>10</factor>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-0.55741</x-m>
			<z-m>-5.20529</z-m>
                    </center>
	</animation>
        <!--Gear retraction-->
    <animation>
		<type>rotate</type>
		<object-name>wheelF</object-name>
		<object-name>gearFU</object-name>
		<object-name>gearFUdamper</object-name>
		<object-name>gearFD</object-name>
		<object-name>gearFDdamper</object-name>
                <property>gear/gear/position-norm</property>
		<offset-deg>108</offset-deg>
		<factor>-108</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
                <center>
			<x-m>-15.75171</x-m>
			<z-m>-3.72991</z-m>
                    </center>
	</animation>
        
    <animation>
		<type>rotate</type>
		<object-name>wheelL</object-name>
		<object-name>gearLU</object-name>
		<object-name>gearLUdamper</object-name>
		<object-name>gearLD</object-name>
		<object-name>gearLDdamper</object-name>
                <property>gear/gear/position-norm</property>
		<offset-deg>90</offset-deg>
		<factor>-90</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
                <center>
			<y-m>-2.36542</y-m>
			<z-m>-3.32251</z-m>
                    </center>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>wheelR</object-name>
		<object-name>gearRU</object-name>
		<object-name>gearRUdamper</object-name>
		<object-name>gearRD</object-name>
		<object-name>gearRDdamper</object-name>
                <property>gear/gear/position-norm</property>
		<offset-deg>-90</offset-deg>
		<factor>90</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
                <center>
			<y-m>2.36542</y-m>
			<z-m>-3.32251</z-m>
                    </center>
	</animation>
        <!--CABIN-->
        <!--Seats-->
    <animation>
		<type>pick</type>
		<object-name>mixedclass1</object-name>
		<object-name>mixedclass2</object-name>
		<object-name>mixedclass3</object-name>
		<object-name>mixedclass4</object-name>
		<object-name>standard1</object-name>
		<object-name>standard2</object-name>
		<object-name>highdensity1</object-name>
		<object-name>highdensity2</object-name>
                <action>
                    <button>0</button>
                    <binding>
                        <command>property-cycle</command>
                <property>/controls/cabin/seating</property>
                <value>0</value>
                <value>1</value>
                <value>2</value>
            </binding>
        </action>
	</animation>
    <animation>
		<type>select</type>
		<object-name>mixedclass1</object-name>
		<object-name>mixedclass2</object-name>
		<object-name>mixedclass3</object-name>
		<object-name>mixedclass4</object-name>
                <condition><equals>
                <property>/controls/cabin/seating</property>
                <value>0</value></equals>
                </condition>
	</animation>
    <animation>
		<type>select</type>
		<object-name>standard1</object-name>
		<object-name>standard2</object-name>
                <condition><equals>
                <property>/controls/cabin/seating</property>
                <value>1</value></equals>
                </condition>
	</animation>
    <animation>
		<type>select</type>
		<object-name>highdensity1</object-name>
		<object-name>highdensity2</object-name>
                <condition><equals>
                <property>/controls/cabin/seating</property>
                <value>2</value></equals>
                </condition>
	</animation>
</PropertyList>
